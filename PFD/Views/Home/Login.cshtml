@{
    ViewData["Title"] = "Login";
    
}
<!DOCTYPE html>
<html>
<head>
    <title>Gathering Log In form Responsive Widget Template :: W3layouts</title>
    <!-- Meta tag Keywords -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8" />
    <!-- //Meta tag Keywords -->
    <link href="//fonts.googleapis.com/css2?family=Kumbh+Sans:wght@300;400;700&display=swap" rel="stylesheet">
    <!--/Style-CSS -->
    <link rel="stylesheet" href="~/css/style.css" type="text/css" media="all" />
    <!--//Style-CSS -->
    <link rel="stylesheet" href="~/css/font-awesome.min.css" type="text/css" media="all">

</head>
<body>
    <div class="signinform">
        <!-- container -->
        <div class="wrapper">
            <!-- main content -->
            <div class="form-info">
                <div class="info">
                    <h1>Online Banking</h1>
                    <h2>Log In</h2>
                    <form asp-action="Login" method="post">
                        <div class="input-group">
                            <span><i class="fa fa-user" aria-hidden="true"></i></span>
                            <input name="memberlogin" id="memberlogin" type="email" placeholder="Email or Username" required="">
                        </div>
                        <div class="input-group two-groop">
                            <span><i class="fa fa-key" aria-hidden="true"></i></span>
                            <input name="memberpassword" type="Password" id="memberpassword" placeholder="Password" required="">
                        </div>
                        <div class="form-row bottom">
                            <div class="form-check">
                                <input type="checkbox" id="remenber" name="remenber" value="remenber">
                                <label for="remenber"> Remember me?</label>
                            </div>
                            <a href="#url" class="forgot">Forgot password?</a>
                        </div>
                        <button class="btn btn-primary btn-block" type="submit">Log In</button>
                    </form>

                    <p class="account">Don't have an account? <a href="#register">Register</a></p>
                </div>
            </div>
            <!-- //main content -->
        </div>
        <!-- //container -->
        
    </div>
</body>
</html>
